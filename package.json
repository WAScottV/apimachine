{
  "name": "apimachine",
  "version": "0.5.0",
  "description": "API test runner for microservices",
  "main": "server.js",
  "author": "Strategic Machines",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-register": "^6.26.0",
    "supertest": "^3.1.0"
